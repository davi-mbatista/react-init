    "parser": "babel-eslint",
    "plugins": ["babel", "prettier", "jsx-a11y"],
    "extends": ["eslint:recommended", "prettier", "react-app", "plugin:jsx-a11y/recommended"],
    "rules": { "prettier/prettier": "warn" },
    "env": {
        "browser": true,
        "es6": true,
